{
  "name": "cl-utils",
  "version": "0.1.5",
  "description": "Most common used utility for web develop",
  "main": "./libs/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "start": "babel ./src --no-comments --delete-dir-on-start -d ./libs -x .ts,.tsx -w",
    "types": "node ./scripts/types.js",
    "build:ts": "babel ./src --no-comments --delete-dir-on-start -d ./libs -x .ts,.tsx",
    "build": "npm run types && npm run build:ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joye61/cl-utils.git"
  },
  "keywords": [
    "library",
    "react"
  ],
  "author": "joye",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joye61/cl-utils/issues"
  },
  "homepage": "https://github.com/joye61/cl-utils#readme",
  "registry": "https://registry.npmjs.org",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@emotion/babel-preset-css-prop",
        {
          "sourceMap": false
        }
      ]
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./__tests__/__coverage"
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "axios": "^0.19.0",
    "emotion": "^10.0.14",
    "lodash": "^4.17.15",
    "qs": "^6.8.0",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.137",
    "@types/node": "^12.7.2",
    "@types/qs": "^6.5.3",
    "@types/raf": "^3.4.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "typescript": "^3.5.3"
  }
}
